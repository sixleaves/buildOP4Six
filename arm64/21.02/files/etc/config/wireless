
config wifi-device 'MT7986_1_1'
	option type 'mtwifi'
	option phy 'ra0'
	option band '2g'
	option dbdc_main '1'
	option channel 'auto'
	option txpower '100'
	option htmode 'HE40'
	option mu_beamformer '1'
	option noscan '1'
	option serialize '1'
	option country 'AU'

config wifi-iface 'default_MT7986_1_1'
	option device 'MT7986_1_1'
	option mode 'ap'
	option ssid 'sweetcs-iot'
	option encryption 'sae-mixed'
	option key '88ffeac3!'
	option network 'iot'

config wifi-device 'MT7986_1_2'
	option type 'mtwifi'
	option phy 'rax0'
	option band '5g'
	option dbdc_main '0'
	option channel 'auto'
	option txpower '100'
	option htmode 'HE160'
	option mu_beamformer '1'
	option serialize '1'
	option country 'AU'

config wifi-iface 'default_MT7986_1_2'
	option device 'MT7986_1_2'
	option mode 'ap'
	option ssid 'misaav-5G'
	option encryption 'sae-mixed'
	option key '88ffeac3!'
	option network 'wifi'

